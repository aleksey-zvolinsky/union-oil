  object qGrid: TOilQuery
    Session = frmStart.OraSession1
    SQL.Strings = (
      'select npt.name as np_type,'
      '       npt.id as np_type_id,'
      '       npt.us_subgroup_name,'
      '       p.ss,'
      '       p.dog,'
      '       Q.part_id,'
      '       Q.part_inst,'
      '       p.plan_id,'
      '       p.own_goods,'
      '       '#39'Y'#39' as state,'
      
        '       decode(sum(Q.price), 0, nvl(vlp.price, p.price), sum(Q.pr' +
        'ice)) as price,'
      '       oo.id as post_id,'
      '       oo.inst as post_inst,'
      '       oo.name as postavschik,'
      '       sum(Q.id) as id,'
      '       sum(Q.rest) as rest,'
      '       sum(Q.shtuki) as shtuki,'
      '       npt.codavias,'
      
        '       to_number(ov.GetOutLinkVal('#39'OIL_NP_TYPE'#39', 2, npt.id)) as ' +
        'codoptima,'
      '       to_number(ov.GetOutLinkVal('#39'OIL_NP_TYPE'#39','
      '                                  ov.GetVal('#39'NP_CODINT'#39'),'
      '                                  npt.id)) as codint'
      '  from (select to_number(null) as id,'
      '               0 as price,'
      '               part_id,'
      '               part_inst,'
      '               sum(tonn) as rest,'
      '               0 as shtuki'
      
        '          from (select pr.part_id, pr.part_inst, prd.fact_count ' +
        'as tonn'
      '                  from oil_prihod pr, oil_prih_det prd'
      '                 where pr.state = '#39'Y'#39
      '                   and prd.state = '#39'Y'#39
      '                   and prd.prihod_id = pr.id'
      '                   and prd.inst = pr.inst'
      '                   and pr.inst = :p_Inst'
      '                   and pr.date_ <= :p_Date'
      '                   and prd.rez = :p_RezId'
      '                   and prd.rez_inst = :p_RezInst'
      '                union all'
      '                select part_id, part_inst, -count_ as tonn'
      '                  from oil_rashod'
      '                 where state = '#39'Y'#39
      '                   and inst = :p_Inst'
      '                   and rez = :p_RezId'
      '                   and rez_inst = :p_RezInst'
      
        '                   and (group_num <> :p_GroupNumDel or group_num' +
        ' is null))'
      '         group by part_id, part_inst'
      '        union all'
      '        select r.id,'
      '               r.price,'
      '               r.part_id,'
      '               r.part_inst,'
      '               0           as rest,'
      '               count_      as shtuki'
      '          from oil_rashod r'
      '         where state = '#39'Y'#39
      '           and inst = :p_Inst'
      '           and group_num = :p_GroupNum'
      '           and date_ = :p_StartDate) Q,'
      '       oil_part p,'
      '       v_oil_last_price vlp,'
      '       oil_org oo,'
      '       v_oil_np_type npt,'
      '       v_oil_dim dim'
      ' where Q.part_id = p.id'
      '   and Q.part_inst = p.inst'
      '   and not (Q.shtuki <= 0 and Q.rest <= 0)'
      '   and p.state = '#39'Y'#39
      '   and Q.part_id = vlp.part_id(+)'
      '   and Q.part_inst = vlp.part_inst(+)'
      '   and vlp.inst(+) = :p_Inst'
      '   and p.from_ = oo.id'
      '   and p.from_inst = oo.inst'
      '   and p.np_type = npt.id'
      '   and p.dim_id = dim.id'
      '   and dim.IsShtuki = '#39'Y'#39
      '   and (p.soputst = '#39'Y'#39' or npt.us_subgroup_id is not null)'
      ' group by npt.name,'
      '          npt.id,'
      '          p.ss,'
      '          p.dog,'
      '          Q.part_id,'
      '          Q.part_inst,'
      '          p.plan_id,'
      '          npt.us_subgroup_name,'
      '          p.own_goods,'
      '          oo.id,'
      '          oo.inst,'
      '          oo.name,'
      '          p.price,'
      '          vlp.price,'
      '          npt.codavias,'
      '          to_number(ov.GetOutLinkVal('#39'OIL_NP_TYPE'#39', 2, npt.id))'
      ' order by npt.sortname, dog, npt.name')
    RequestLive = False
    UpdateMode = upWhereAll
    Left = 16
    Top = 144
    ParamData = <
      item
        DataType = ftInteger
        Name = 'p_Inst'
        ParamType = ptInput
      end
