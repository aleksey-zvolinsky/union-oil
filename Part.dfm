  object qAfterSave: TOraQuery
    Session = frmStart.OraSession1
    SQL.Strings = (
      'begin'
      '  update oil_rashod'
      '     set to_id = :AFromId, to_inst = :AFromInst,'
      '         from_id = :AToId, from_inst = :AToInst'
      '   where oper_id = 18'
      '     and state = '#39'Y'#39
      '     and part_id = :APartId'
      '     and part_inst = :APartInst'
      
        '/*     and not (from_id = :AFromId and from_inst = :AFromInst an' +
        'd to_id = :AToId and to_inst = :AToInst)*/;'
      ''
      '  update oil_prihod'
      '     set from_ = :AToId, from_inst = :AToInst'
      '   where oper_id = 18'
      '     and state = '#39'Y'#39
      '     and part_id = :APartId'
      '     and part_inst = :APartInst'
      '/*     and not (from_ = :AFromId and from_inst = :AFromInst)*/;'
      ''
      '  update oil_prihod'
      '     set to_ = :AFromId, to_inst = :AFromInst'
      '   where oper_id = 18'
      '     and state = '#39'Y'#39
      '     and (id, inst) in (select prih, prih_inst'
      '                          from oil_rashod'
      '                         where oper_id = 18'
      '                           and state = '#39'Y'#39
      '                           and part_id = :APartId'
      '                           and part_inst = :APartInst);'
      'end;')
    Left = 40
    Top = 288
    ParamData = <
      item
        DataType = ftInteger
        Name = 'AFromId'
      end
      item
        DataType = ftInteger
        Name = 'AFromInst'
      end
      item
        DataType = ftInteger
        Name = 'AToId'
      end
      item
        DataType = ftInteger
        Name = 'AToInst'
      end
      item
        DataType = ftInteger
        Name = 'APartId'
      end
