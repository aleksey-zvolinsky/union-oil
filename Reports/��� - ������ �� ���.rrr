[GLOBAL]
   TITLE = Приход на АЗС@Прихiд на АЗС
   MENU = Отчеты\Отчеты по АЗС\Приход на АЗС@Прихiд на АЗС
   MENU_POSITION = 2
   VERSION = 1.4
   RAZM_SYMBOLS = NO
   PAGES_WIDTH = 1

[PANELS]
