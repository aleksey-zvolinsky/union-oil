   CAPTION = Основной без уч.цен@Основний без обл.цін
   ITOGI = podrname,azsname,np_type
   UNCHECKED = np_group,litr,tonn,shtuki,ss,bnal_summa,nal_summa,tal_summa,bank_summa,card_summa,card_auto_summa,summa,nds