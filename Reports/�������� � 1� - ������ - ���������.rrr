[GLOBAL]
  TITLE = Талоны - проданные@Талони - продані
  VERSION = 1.0
  MENU = Отчеты\Выгрузка в 1С\Талоны - проданные@Талони - продані
  RAZM_SYMBOLS = NO
  PAGES_WIDTH = 1

[PANELS]
  DATEPANEL
  END

[DESCRIPTION]
  От :BeginDate по :EndDate@Від :BeginDate до :EndDate

[SQL]
 select v.from_id as filial_id,
       nvl(oo.id_num, odet2.fake_id_num) as id_num,
       oo.nal_num as inn,
       v.ser,
       v.nominal,
       count(*) as amount,
       v.price,
       dog.dog,
       dog.dog_date,
       trunc(v.date_) as rep_date,
       decode(sign(5e8 - v.num), 1, 0, 1) as old_new,
       oo.name as org_name,
       decode(v.oper_id, 245, 1, 0) as sp_help
  from (select v.*,
               lead(oper_id) over(partition by ser, num order by date_, tip) as next_oper_id
          from v_oil_talon_op v) v,
       oil_org oo,
       oil_org_det2 odet2,
       oil_dog dog
 where v.tip = 'O'
   and v.to_id = oo.id
   and v.to_inst = oo.inst
   and odet2.state(+) = 'Y'
   and odet2.org_id(+) = oo.id
   and odet2.org_inst(+) = oo.inst
   and v.dog_id = dog.id(+)
   and v.dog_inst = dog.inst(+)
   and dog.state(+) = 'Y'
   and nvl(next_oper_id, 0) <> 181
   and nvl(oper_id, 0) not in (192, 191)
   and v.date_ between :BeginDate and :EndDate
 group by v.from_id,
          nvl(oo.id_num, odet2.fake_id_num),
          v.oper_id,
          oo.nal_num,
          v.ser,
          v.nominal,
          decode(sign(5e8 - v.num), 1, 0, 1),
          v.price,
          dog.dog,
          dog.dog_date,
          v.date_,
          oo.name
 order by rep_date

