   WIDTH = 10
   FORMAT = dd.mm.yyyy

   NAME = nominal
   CAPTION = Номинал@Номінал
   WIDTH=15

   NAME = ser
   CAPTION = Серия@Серія
   WIDTH = 6

   NAME = num
   CAPTION = Номер
   WIDTH = 8

   NAME = price
   CAPTION = Цена@Ціна
   WIDTH = 10
   FORMAT = 0.00

   NAME = shtuki
   CAPTION = Кол-во@К-сть
   WIDTH = 8
   FORMAT = 0
   TOSUM = TRUE

   NAME = litr
   CAPTION = Литры@Літри
   WIDTH = 10
   FORMAT = 0
   TOSUM = TRUE

   NAME = summa
   CAPTION = Сумма (грн)@Сума (грн)
   WIDTH = 12
   FORMAT = 0.00
   TOSUM = TRUE

