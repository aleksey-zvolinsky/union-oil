[TYPES]

   CAPTION = Группировка по группам НП, партиям, АЗС@Группування по групам НП, партіям, АЗС
   ITOGI = GroupName,Dog,name
   UNCHECKED = GroupName_filtr3

   CAPTION = Группировка по группам НП, АЗС@Группування по групам НП, АЗС
   ITOGI = GroupName,name
   UNCHECKED = GroupName_filtr3

   CAPTION = АЗС, Группировка по группам НП@АЗС, Группування по групам НП
   ITOGI = name,GroupName_filtr3
   UNCHECKED = GroupName