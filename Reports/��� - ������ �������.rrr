  NAME = cnt
  CAPTION = Кол.номеров@К-сть номерів
  WIDTH = 15

  NAME = date_
  CAPTION = Дата
  WIDTH = 15
  
  NAME = num
  CAPTION = Номер
  WIDTH = 15

  NAME = type_
  CAPTION = Тип
  WIDTH = 30

