[GLOBAL]
  TITLE = Резерв номеров@Резерв номерів
  MENU = Отчеты\Налоговые накладные\Резерв номеров@Резерв номерів
  VERSION = 1.0
  RAZM_SYMBOLS = NO
  PAGES_WIDTH = 1
  PAGE_ORIENT = GOR
  SHOW_INFO = NO


[PANELS]

  DATEPANEL
  END

  ORGPANEL
  END


[DESCRIPTION] 

  \{A5}MОрганизация :org1.name@\{A5}MОрганізація :org1.name
  \{A6}MПериод с :BeginDate по :EndDate@\{A6}MПеріод з :BeginDate по :EndDate

[SQL]

select r.date_,
       r.num,
       nvl(t.name_,'Налогoвая') type_,
       count(*) cnt
  from oil_nnum_reserve r,
       oil_nn_type t
 where r.state = 'Y'
   and t.id(+) = r.nn_type_id
   and inst = :Org1.ID
   and date_ between :BeginDate and :EndDate
group by r.date_,
       r.num,
       nvl(t.name_,'Налогoвая')

