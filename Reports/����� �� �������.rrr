   TOSUM = TRUE
   OPTIONAL = FALSE

   NAME = nds
   CAPTION = НДС@ПДВ
   WIDTH = 15
   FORMAT = 0.00
   TOSUM = TRUE
   OPTIONAL = FALSE

   NAME = summnds
   CAPTION = Сумма c НДС@Сума з ПДВ
   WIDTH = 15
   FORMAT = 0.00
   TOSUM = TRUE
   OPTIONAL = FALSE

[TYPES]
   
