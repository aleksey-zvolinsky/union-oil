[GLOBAL]
  TITLE = Реестр расходных по нефтепродуктам (Филиал и ОАО)@Реєстр видаткових за нафтопродуктами (Філія і ВАТ)
  MENU = Отчеты\Отчеты по талонам\Филиальный пакет\Реестр по нефтепродуктам@Реєстр за нафтопродуктам
  VERSION = 1.0
  RAZM_SYMBOLS = NO
  PAGES_WIDTH = 1

[PANELS]

  DATEPANEL
  END

[DESCRIPTION]
  От :BeginDate по :EndDate@Від :BeginDate до :EndDate

[SQL]
select
  date_,
  azs_name,
  np_group,
  litr, price,
  sumxnds, nds, sumnds
from v_oil_talon_srae ae
where is_np_docs=1
  and date_ between :BeginDate and :EndDate


[FIELDS]

  NAME = DATE_
  CAPTION = Дата
  WIDTH = 10

  NAME = NP_GROUP
  CAPTION = Группа НП@Група НП
  WIDTH = 10
  
  NAME = AZS_NAME
  CAPTION = АЗС
  WIDTH = 15

  NAME = PRICE
  CAPTION = Цена с НДС@Ціна з ПДВ
  WIDTH = 10
  FORMAT = 0.00
  
  DEFAULT TOSUM = TRUE
  DEFAULT OPTIONAL = FALSE

  NAME = LITR
  CAPTION = Литров@ЛІтрів
  WIDTH = 10
  FORMAT = 0
  
  DEFAULT FORMAT = 0.00

  NAME = SUMXNDS
  CAPTION = Сумма без НДС@Сума без ПДВ
  WIDTH = 10

  NAME = NDS
  CAPTION = НДС
  WIDTH = 10

  NAME = SUMNDS
  CAPTION = Сумма с НДС@Сума з ПДВ
  WIDTH = 10

[TYPES]
  CAPTION = Основной@Основний
  ITOGI = DATE_,NP_GROUP
