VERSION = 2.2
RAZM_SYMBOLS = NO
PAGES_WIDTH = 1
MENU = Отчеты\Отчеты по ОП\Расшифровка ОП в инкассационном чеке на АЗС@Розшифровка ОП в инкассационном чеці на АЗС

[PANELS]
DATEPANEL
END

ORGPANEL
OPTIONS1=nnyyyynn
STARTCONDITION1=0
END

[VARIABLES]
VAR FADDR:varSql
select azs.addr
from card_modem_call cmc,
oil_org azs
where oil_inst = azs.par and state = 'Y'
and azs.id = cmc.azs_id
and azs.id = :org1.id
group by cmc.azs_id,oil_inst,azs.addr
END
VAR FCALL_DATE:varSql
select to_char(max(call_date),'DD.MM.YYYY HH24:MI:SS')call_date
from card_modem_call cmc,
oil_org azs
where oil_inst = azs.par and state = 'Y'
and azs.id = cmc.azs_id
and azs.id = :org1.id
group by cmc.azs_id,oil_inst,azs.addr
END

[DESCRIPTION_RUS]

\{B6}MПериод с \{C6}M\B:BeginDate\b \{D6}Mпо \{E6}M\B:EndDate\b
\{B7}MАЗС :: \{C7}M\B:org1.Name\b
\{B8}Mадрес :: \{C8}M\B:FADDR\b
\{B9}Mпоследняя дата инкасации :: \{C9}M\B:FCALL_DATE\b

[DESCRIPTION_UKR]

\{B6}MПеріод з \{C6}M\B:BeginDate\b \{D6}Mпо \{E6}M\B:EndDate\b
\{B7}MАЗС :: \{C7}M\B:org1.Name\b
\{B8}Mадреса :: \{C8}M\B:FADDR\b
\{B9}Mостання дата інкасації :: \{C9}M\B:FCALL_DATE\b

[SQL]
select rownum,
op.name_firm,
card_number,
date_create,
datetime_create,
date_end,
name_services,
op.sum_than,
description_condition
from v_card_op op
where op.NUMBER_TERMINAL=0 and op.DESCRIPTION_CONDITION='Отправлено'             and (op.date_create between :BeginDate and :EndDate)

