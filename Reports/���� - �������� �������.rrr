  WIDTH = 8

  NAME = cnt_sale
  CAPTION = Расход!Реализация!Количество@Відпуск!Реалізація!Кількість
  WIDTH = 8

  NAME = price_sale
  CAPTION = Расход!Реализация!Цена@Відпуск!Реалізація!Ціна
  WIDTH = 8

  NAME = summ_sale
  CAPTION = Расход!Реализация!Сумма@Відпуск!Реалізація!Сума
  WIDTH = 8

  NAME = cnt_own
  CAPTION = Расход!Собств. нужды@Відпуск!Власні потреби
  WIDTH = 8

  NAME = cnt_write_off
  CAPTION = Расход!Списано@Відпуск!Списано
  WIDTH = 8

  NAME = cnt_service
  CAPTION = Расход!Служ. нужды@Відпуск!Службові потреби
  WIDTH = 8

  NAME = after_cnt
