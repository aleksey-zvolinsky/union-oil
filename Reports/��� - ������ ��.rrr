[GLOBAL]
  TITLE = РЕЄСТР ОТРИМАНИХ ТА ВИДАНИХ ПОДАТКОВИХ НАКЛАДНИХ@РЕЄСТР ОТРИМАНИХ ТА ВИДАНИХ ПОДАТКОВИХ НАКЛАДНИХ
  MENU = Отчеты\Налоговые накладные\Реестр налоговых@Реєстр податкових
  VERSION = 1.9
  RAZM_SYMBOLS = NO
  PAGES_WIDTH = 1
  PAGE_ORIENT = GOR
  SHOW_INFO = NO


[PANELS]
  DATEPANEL
  END

  ORGPANEL
    NUM = 2
    STARTCONDITION1=0
    STARTCONDITION2=4
  END

   PANEL
       ELEMENT Fnumber:Edit
          CAPTION = Суфикс подразделения@Суфикс підрозділу
       END
   END

   PANEL
      ELEMENT CauseList:List
	  CAPTION = Источник:@Джерело:
          TABLENAME = (select id,name_ name,'Y' state from oil_nn_cause)
	  CONDITION = id <> 111
      END
      ELEMENT TypeList:List
	  CAPTION = Тип:
          TABLENAME = (select id,name_ name,'Y' state from oil_nn_type)
	  CONDITION = id<>111
      END
   END

[DESCRIPTION]
                                                                                       \{I1}MЗатверджено наказом ДПА України
                                                                                       \{I2}Mвід 30.06.2005 № 244
  \{A4}MНазва платника податку :org1.name                                              \{F4}MРеэстр розпочато :BeginDate
  \{A5}MІндивідуальний податковий номер :org1.NAL_NUM                                  \{F5}MРеэстр закінчено :EndDate
  \{A6}MСвідоцтво про реєстрацію платника податку на додану вартість :org1.SVID_NUM    \{F6}MЗберігати_______________________
  \{D7}M\BРозділ ІІ. Видані податкові накладні
AFTER
  \1LВиписав_________________________________

[SQL]

select rownum,sb.*
  from
(select to_char(sb.s_date,'DD.MM.YYYY')s_date,
       substr(sb.grid_num||decode(:FNUMBER,null,fnum.value,:FNUMBER),1,50) num,
       sb.organ_name,
       sb.organ_naln,
       sb.organ_okpo,
       sb.nds_t,
       sb.sum_base nds_base,
       sb.sum_20 nds_20,
       0 as q,
       0 as w,
       0 as e
  from v_oil_sale_book sb,
       (select decode(max(value),'0','',max(value)) as value
          from adm_settings
         where name = 'FILIAL_NUMBER'
           and inst=:org1.inst ) fnum
 where sb.S_DATE between :BeginDate and :EndDate
   and sb.FROM_INST = :org1.inst and sb.FROM_ = :org1.inst
   and ((sb.organ_INST = :org2.inst and sb.organ = :org2.id)or(:org2.id is null))
   and :CauseList.IsInList{sb.nn_cause_id}
   and :TypeList.IsInList{sb.nn_type_id}
 group by sb.s_date,
       to_number(TRANSLATE(upper(sb.num),
                 '0123456789 ABCDEFGHIJKLMNOPQRSTUVWXYZАБВГДЕЖЗИЙКЛМНОПРСТУФХЦЧШЩЪЬЫЄЮЯ.,\/|*-+}{][=~`!@#$%^''"№;%?',
                 '0123456789')),
       sb.grid_num,
       sb.organ_name,
       sb.organ_naln,
       sb.organ_okpo,
       sb.nds_t,
       sb.sum_base,
       sb.sum_20,
       fnum.value )sb

