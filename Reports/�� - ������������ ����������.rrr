[GLOBAL]
   TITLE = Движение по арендованным резервуарам@Рух по арендованих резервуарам
   MENU = Отчеты\Движение нефтепродуктов\Арендованные резервуары@Арендовані резервуари
   MENU_POSITION = 0
   NAME_FOR_SAVE = НБСобстХран
   VERSION = 1.4
   PAGES_WIDTH = 2
   RAZM_SYMBOLS = NO

[PANELS]
   DATEPANEL
   END

   ORGPANEL
   END

   PANEL
      CAPTION = Тип товара
      ELEMENT KOS:CheckBoxGroup
         LIST = Комиссия,Собственный,Хранение@Комісія,Власний,Зберігання
         VALUES = 'K','O','S'
      END
   END

   PANEL
      CAPTION = Стоимость@Вартість
