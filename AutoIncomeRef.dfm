      '  AUTO.NUMB AS NUMB,'
      '  AUTO.AUTO_MODEL AS MODEL,'
      
        '  AUTOGROUP.NAME AS AUTOGROUPNAME, /* '#1053#1040#1047#1042#1040#1053#1048#1045' '#1043#1056#1059#1055#1055#1067' '#1058#1045#1061#1053#1048#1063#1045#1057#1050#1048 +
        #1061' '#1057#1056#1045#1044#1057#1058#1042' */'
      '  AUTOTYPE.NAME AS AUTOTYPENAME,'
      '  INCOME.NP_TYPE,'
      '  NP_TYPE.NAME AS NPNAME, /* '#1053#1040#1047#1042#1040#1053#1048#1045' '#1053#1055' */'
      '  INCOME.PRICE, /* '#1059#1063#1045#1058#1053#1040#1071' '#1062#1045#1053#1040' '#1047#1040' '#1058#1054#1053#1053#1059'*/'
      '  INCOME.LITRES, /* '#1047#1040#1051#1048#1058#1054' '#1051#1048#1058#1056#1054#1042' */'
      '  INCOME.TONNS, /* '#1047#1040#1051#1048#1058#1054' '#1058#1054#1053#1053' */'
      
        '  ROUND(INCOME.PRICE * INCOME.TONNS, 2) AS SUMCOST /* '#1057#1058#1054#1048#1052#1054#1057#1058#1068' ' +
        '*/'
