  inherited q: TOilQuery
    SQL.Strings = (
      'SELECT waybill.id,'
      '       waybill.inst,'
      '       waybill.waybill_date,'
      '       waybill.waybill_number,'
      '       np_type.name AS NPType,'
      '       autogroup.name,'
      '       autotype.name AS Auto_Type,'
      '       auto.numb,'
      '       auto.auto_model,'
      
        '       (employ.f_name || '#39' '#39' || SUBSTR(employ.i_name, 1, 1) || '#39 +
        '.'#39' ||'
      '       SUBSTR(employ.o_name, 1, 1) || '#39'.'#39') AS EmployName,'
      '       SUM(oper.litres) AS litres,'
      '       SUM(oper.kilometers) AS km'
      '  FROM oil_waybills   waybill,'
      '       oil_auto       auto,'
      '       oil_employ     employ,'
      '       oil_auto_group autogroup,'
      '       oil_auto_type  autotype,'
      '       oil_np_type    np_type,'
      '       oil_np_group   np_group,'
      '       oil_auto_oper  oper'
      ' WHERE waybill.id = oper.waybill_id'
      '   AND waybill.inst = oper.waybill_inst'
      '   AND waybill.employ_id = employ.id'
      '   AND waybill.employ_inst = employ.inst'
      '   AND waybill.auto_id = auto.id'
      '   AND waybill.auto_inst = auto.inst'
      '   AND auto.auto_type_id = autotype.id'
      '   AND autotype.auto_group = autogroup.id'
      '   AND waybill.np_type = np_type.id'
      '   AND np_type.grp_id = np_group.id'
      
        '      /* '#1057#1090#1088#1086#1082#1072' 32 - '#1079#1072#1088#1077#1079#1077#1088#1074#1080#1088#1086#1074#1072#1085#1086' '#1076#1083#1103' '#1092#1080#1083#1100#1090#1088#1072' '#1085#1072' waybill.wayb' +
        'ill_date >= */'
      
        '      /* '#1057#1090#1088#1086#1082#1072' 33 - '#1079#1072#1088#1077#1079#1077#1088#1074#1080#1088#1086#1074#1072#1085#1086' '#1076#1083#1103' '#1092#1080#1083#1100#1090#1088#1072' '#1085#1072' waybill.wayb' +
        'ill_date <= */'
      
        '      /* '#1057#1090#1088#1086#1082#1072' 34 - '#1079#1072#1088#1077#1079#1077#1088#1074#1080#1088#1086#1074#1072#1085#1086' '#1076#1083#1103' '#1092#1080#1083#1100#1090#1088#1072' '#1085#1072' waybill.wayb' +
        'ill_number */'
      
        '      /* '#1057#1090#1088#1086#1082#1072' 35 - '#1079#1072#1088#1077#1079#1077#1088#1074#1080#1088#1086#1074#1072#1085#1086' '#1076#1083#1103' '#1092#1080#1083#1100#1090#1088#1072' '#1085#1072' np_group.id ' +
        '*/'
      
        '      /* '#1057#1090#1088#1086#1082#1072' 36 - '#1079#1072#1088#1077#1079#1077#1088#1074#1080#1088#1086#1074#1072#1085#1086' '#1076#1083#1103' '#1092#1080#1083#1100#1090#1088#1072' '#1085#1072' autogroup.id' +
        ' */'
      
        '      /* '#1057#1090#1088#1086#1082#1072' 37 - '#1079#1072#1088#1077#1079#1077#1088#1074#1080#1088#1086#1074#1072#1085#1077' '#1076#1083#1103' '#1092#1080#1083#1100#1090#1088#1072' '#1085#1072' autotype.id ' +
        '*/'
      
        '      /* '#1057#1090#1088#1086#1082#1072' 38 - '#1079#1072#1088#1077#1079#1077#1088#1074#1080#1088#1086#1074#1072#1085#1086' '#1076#1083#1103' '#1092#1080#1083#1100#1090#1088#1072' '#1085#1072' waybill.auto' +
        '_id */'
      
        '      /* '#1057#1090#1088#1086#1082#1072' 39 - '#1079#1072#1088#1077#1079#1077#1088#1074#1080#1088#1086#1074#1072#1085#1086' '#1076#1083#1103' '#1092#1080#1083#1100#1090#1088#1072' '#1085#1072' waybill.auto' +
        '_inst */'
      
        '      /* '#1057#1090#1088#1086#1082#1072' 40 - '#1079#1072#1088#1077#1079#1077#1088#1074#1080#1088#1086#1074#1072#1085#1086' '#1076#1083#1103' '#1092#1080#1083#1100#1090#1088#1072' '#1085#1072' waybill.empl' +
        'oy_id */'
      
        '      /* '#1057#1090#1088#1086#1082#1072' 41 - '#1079#1072#1088#1077#1079#1077#1088#1074#1080#1088#1086#1074#1072#1085#1086' '#1076#1083#1103' '#1092#1080#1083#1100#1090#1088#1072' '#1085#1072' waybill.empl' +
        'oy_inst */'
      '   AND oper.state = '#39'Y'#39
      '   AND waybill.state = '#39'Y'#39
      ' GROUP BY waybill.id,'
      '          waybill.inst,'
      '          waybill.waybill_date,'
      '          waybill.waybill_number,'
      '          np_type.name,'
      '          autogroup.name,'
      '          autotype.name,'
      '          auto.numb,'
      '          auto.auto_model,'
      '          employ.f_name,'
      '          employ.i_name,'
