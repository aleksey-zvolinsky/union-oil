  inherited q: TOilQuery
    SQL.Strings = (
      'select /*+ORDERED USE_HASH(tr, OrgWhere, OrgWho, AZS, sb)*/'
      '  tr.Oper_date,'
      '  tr.Card_Number,'
      '  np.name as NpName,'
      '  purse.name as PurseName,'
      '  OrgWhere.Name as OrgWhere,'
      '  OrgWho.Name as OrgWho,'
      '  AZS.Name as AZS,'
      '  tr.Count_Litr,'
      '  tr.Count_Purse,'
      '  tr.Term_Price,'
      '  tr.Recive_Date,'
      '  tr.Send_Date,'
      '  tr.oil_rn_num,'
      '  tr.oil_rn_date,'
      '  tr.oil_schet_num,'
      '  tr.oil_schet_date,'
      '  sb.num as oil_nn_num,'
      '  sb.s_date as oil_nn_date,'
      '  tr.em_where,'
      '  tr.em_who,'
      '  orgWho.id  as OrgId,'
      '  orgWho.inst as OrgInst,'
      '  azs.id as AzsId,'
      '  azs.inst as AzsInst'
      'from'
      '  card_transaction_money tr,'
      '  v_org OrgWhere,'
      '  v_org OrgWho,'
      '  v_org AZS,'
      '  oil_sale_book sb,'
      '  oil_np_group np,'
      '  oil_np_group purse'
      'where'
      '  tr.terminal_number+1000000=azs.id(+) and'
      
        '  tr.terminal_number+1000000 = decode(azs.inst(+),1,azs.id(+),az' +
        's.inst(+)) and'
      '  tr.Em_where = OrgWhere.Card_Id (+) and'
      '  tr.Em_Who = OrgWho.Card_Id (+) and'
      '  tr.Np_Type = np.petrol_id (+) and'
      '  tr.Purse = purse.petrol_id (+) and'
      '  tr.oil_nn_id = sb.id (+) and'
      '  tr.oil_nn_inst = sb.Inst (+) and'
      
        '  (OrgWho.Name like '#39'%'#39' || :OrgName || '#39'%'#39' or :OrgName is null) ' +
        'and'
      
        '  (OrgWhere.Name like '#39'%'#39' || :DepName || '#39'%'#39' or :DepName is null' +
        ') and'
      '  trunc(tr.Oper_Date) between :BeginDate and :EndDate and'
